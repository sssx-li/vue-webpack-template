    "stylelint-prettier": "^3.0.0",
    "svg-sprite-loader": "^6.0.11",
    "typescript": "~4.5.5",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-icons": "^0.15.3",
    "unplugin-vue-components": "^0.24.0",
